{
    "name": "Python Dev Container",
    "build": {
      "dockerfile": "Dockerfile",
      "context": ".."
    },
    "extensions": [
      "ms-python.python",
      "ms-python.vscode-pylance"
    ],
    "settings": {
      "python.pythonPath": "/usr/local/bin/python"
    },
    "workspaceFolder": "/workspace"
  }